  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Ufuck1903/birlesikodeme/issues"
  },
  "homepage": "https://github.com/Ufuck1903/birlesikodeme#readme",
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "jest": "^28.1.3",
    "prettier": "^2.8.8",
    "ts-jest": "^28.0.7",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "axios": "^1.4.0"
  }
}
