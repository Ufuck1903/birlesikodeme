{
  "name": "birlesikodeme",
  "version": "1.0.0",
  "description": "nited Payment ödeme API’si, Internet üzerinden ödeme kabul eden tüm işyerlerinin ihtiyacına cevap verecek şekilde, oturum bilgisi tutmayan (stateless) Restful servis odaklı bir mimaride geliştirilmiştir.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ufuck1903/birlesikodeme.git"
  },
  "keywords": [
    "payment",
    "gateway",
    "processing",
    "integration",
    "API",
    "online payments",
    "transactions",
    "credit card",
    "authorization",
    "inquiry",
    "commission",
    "installment",
    "card management",
    "history",
    "merchant",
    "BIN",
    "3D secure",
    "customer cards",
    "client",
    "TypeScript"
  ],
  "author": "Ufuck1903",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Ufuck1903/birlesikodeme/issues"
  },
  "homepage": "https://github.com/Ufuck1903/birlesikodeme#readme",
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.7",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "axios": "^1.4.0"
  }
}
